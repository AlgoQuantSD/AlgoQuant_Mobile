{
  "expo": {
    "name": "AlgoQuant_Mobile",
    "slug": "AlgoQuant_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_json_icons/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/app_json_icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_json_icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/app_json_icons/favicon.png"
    }
  }
}
